    <script>
        function printPDF() {
            $('#printPDF').hide()
            const element = document.getElementById('cv');
            const opt = {
                filename: 'Tran_Thi_Thu_Hien_QC.pdf',
                image: { type: 'jpeg', quality: 0.98 },
                html2canvas: { scale: 2 },
                jsPDF: { unit: 'in', format: 'letter', orientation: 'portrait' }
            };

            html2pdf().set(opt).from(element).save();
            setTimeout(() => {
                $('#printPDF').show()   
            });
        }
    </script>
